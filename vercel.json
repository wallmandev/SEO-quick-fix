{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "cd Desktop/Utveckling/SEO-quick.fix && npm run build",
  "outputDirectory": "Desktop/Utveckling/SEO-quick.fix/dist",
  "installCommand": "cd Desktop/Utveckling/SEO-quick.fix && npm install"
}